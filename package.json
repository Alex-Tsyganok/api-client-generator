{
  "name": "api-client-generator",
  "version": "3.0.0-alpha.1",
  "description": "Angular REST API client generator from Swagger YAML or JSON file with camel case settigs",
  "homepage": "https://github.com/flowup/api-client-generator/blob/master/README.md",
  "main": "src/main.ts",
  "author": {
    "name": "FlowUp",
    "email": "hello@flowup.cz",
    "url": "https://flowup.cz"
  },
  "bin": {
    "api-client-generator": "./dist/main.js"
  },
  "files": [
    "dist/generator.js",
    "dist/generator.js.map",
    "dist/main.js",
    "dist/main.js.map",
    "LICENSE",
    "README.md",
    "templates/ngx-model.mustache",
    "templates/ngx-models-export.mustache",
    "templates/ngx-module-export.mustache",
    "templates/ngx-service.mustache"
  ],
  "bugs": {
    "url": "https://github.com/flowup/api-client-generator/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:flowup/api-client-generator.git"
  },
  "keywords": [
    "swagger",
    "rest",
    "angular5",
    "angular4",
    "angular",
    "ngx",
    "scaffolding",
    "webapi",
    "api",
    "apiclient",
    "generator",
    "camelCase",
    "http",
    "httpclient"
  ],
  "engines": {
    "node": ">=8.9.1",
    "yarn": ">=1.3.2"
  },
  "license": "MIT",
  "scripts": {
    "generate": "node ./dist/main.js",
    "build": "rimraf dist && tsc",
    "lint": "tslint -p tsconfig.json -c tslint.json 'src/**/*.ts'",
    "gen": "ts-node ./src/main.ts",
    "gen-sports": "rimraf ./examples/sports/api && ts-node ./src/main.ts -s ./examples/sports/swagger.yaml -o ./examples/sports/api",
    "gen-esquare": "rimraf ./examples/esquare/api && ts-node ./src/main.ts -s ./examples/esquare/swagger.yaml -o ./examples/esquare/api",
    "gen-esquare-min": "rimraf ./examples/esquare-min/api && ts-node ./src/main.ts -s ./examples/esquare-min/swagger.yaml -o ./examples/esquare-min/api",
    "gen-google-cloud-firestore": "rimraf ./examples/google-cloud-firestore/api && ts-node ./src/main.ts -s ./examples/google-cloud-firestore/swagger.yaml -o ./examples/google-cloud-firestore/api",
    "release": "npm run build && standard-version --tag-prefix=''"
  },
  "dependencies": {
    "mkdirp": "^0.5.1",
    "mustache": "^2.3.0",
    "optimist": "^0.6.1",
    "swagger-parser": "^4.0.1"
  },
  "devDependencies": {
    "@types/mkdirp": "^0.5.2",
    "@types/mustache": "^0.8.30",
    "@types/node": "^9.3.0",
    "@types/optimist": "0.0.29",
    "@types/swagger-parser": "^4.0.2",
    "@types/swagger-schema-official": "^2.0.9",
    "rimraf": "^2.5.2",
    "standard-version": "^4.3.0",
    "swagger-schema-official": "^2.0.0-bab6bed",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-immutable": "^4.5.2",
    "typescript": "^2.7.2"
  }
}
